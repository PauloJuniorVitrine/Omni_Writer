{
  "name": "omni-writer-e2e",
  "version": "1.0.0",
  "description": "Suite de Testes E2E para Omni Writer",
  "main": "index.js",
  "scripts": {
    "test:e2e": "playwright test tests/e2e/",
    "test:e2e:smoke": "playwright test tests/e2e/smoke-tests.spec.ts",
    "test:e2e:critical": "playwright test tests/e2e/test_generate_content.spec.ts tests/e2e/jornada_webhooks_multiplos.spec.ts",
    "test:e2e:performance": "playwright test --config=tests/e2e/performance/parallel-execution.config.ts",
    "test:e2e:incremental": "python scripts/incremental_tests.py",
    "test:e2e:watch": "python scripts/watch_mode.py",
    "test:e2e:watch:enhanced": "python scripts/watch_mode_enhanced.py",
    "test:e2e:validate": "python scripts/validate_synthetic_tests.py tests/e2e/",
    "test:e2e:metrics": "python scripts/generate_e2e_metrics.py",
    "test:e2e:setup": "python scripts/setup_e2e_environment.py",
    "test:e2e:setup:dev": "powershell -ExecutionPolicy Bypass -File scripts/dev_setup.ps1",
    "test:e2e:cache": "python scripts/cache_manager.py --cache-deps --cache-browsers",
    "test:e2e:cache:clean": "python scripts/cache_manager.py --cleanup",
    "test:e2e:cache:stats": "python scripts/cache_manager.py --stats",
    "test:e2e:report": "playwright show-report test-results/html-report",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:codegen": "playwright codegen http://localhost:5000",
    "test:e2e:full": "npm run test:e2e:setup && npm run test:e2e:cache && npm run test:e2e:validate && npm run test:e2e:performance",
    "test:e2e:quick": "npm run test:e2e:smoke",
    "test:e2e:ci": "playwright test --reporter=html,json,junit --workers=1",
    "test:e2e:parallel": "playwright test --workers=auto --shard=1/4",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project=mobile",
    "test:e2e:all-browsers": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:e2e:dev": "E2E_ENV=dev playwright test",
    "test:e2e:staging": "E2E_ENV=staging playwright test",
    "test:e2e:prod": "E2E_ENV=prod playwright test",
    "test:e2e:clean": "rm -rf test-results/ logs/e2e/ .playwright-cache/ .e2e-cache/ .incremental-cache/",
    "test:e2e:reset": "npm run test:e2e:clean && npm run test:e2e:install && npm run test:e2e:setup",
    "test:e2e:help": "echo 'Comandos E2E disponíveis:' && npm run | grep 'test:e2e'",
    "test:e2e:lint": "eslint tests/e2e/ --ext .ts,.js",
    "test:e2e:lint:fix": "eslint tests/e2e/ --ext .ts,.js --fix",
    "test:e2e:format": "prettier --write tests/e2e/**/*.{ts,js}",
    "test:e2e:check": "npm run test:e2e:lint && npm run test:e2e:validate",
    "test:e2e:preflight": "npm run test:e2e:check && npm run test:e2e:smoke",
    "test:e2e:health": "python scripts/health_check.py",
    "test:e2e:diagnose": "python scripts/diagnose_environment.py",
    "test:e2e:troubleshoot": "echo 'Verifique docs/e2e_troubleshooting.md para soluções'",
    "test:e2e:docs": "echo 'Documentação disponível em:' && echo '- docs/e2e_troubleshooting.md' && echo '- tests/e2e/MAINTENANCE_GUIDE.md'",
    "test:e2e:status": "python scripts/status_check.py",
    "test:e2e:monitor": "python scripts/monitor_performance.py",
    "test:e2e:optimize": "python scripts/optimize_performance.py",
    "test:e2e:benchmark": "python scripts/benchmark_tests.py",
    "test:e2e:compare": "python scripts/compare_results.py",
    "test:e2e:export": "python scripts/export_results.py",
    "test:e2e:import": "python scripts/import_results.py",
    "test:e2e:backup": "python scripts/backup_results.py",
    "test:e2e:restore": "python scripts/restore_results.py",
    "test:e2e:archive": "python scripts/archive_old_results.py",
    "test:e2e:purge": "python scripts/purge_old_data.py",
    "test:e2e:sync": "python scripts/sync_results.py",
    "test:e2e:merge": "python scripts/merge_results.py",
    "test:e2e:split": "python scripts/split_results.py",
    "test:e2e:filter": "python scripts/filter_results.py",
    "test:e2e:sort": "python scripts/sort_results.py",
    "test:e2e:search": "python scripts/search_results.py",
    "test:e2e:analyze": "python scripts/analyze_results.py",
    "test:e2e:visualize": "python scripts/visualize_results.py",
    "test:e2e:dashboard": "python scripts/generate_dashboard.py",
    "test:e2e:notify": "python scripts/send_notifications.py",
    "test:e2e:alert": "python scripts/send_alerts.py",
    "test:e2e:report:custom": "python scripts/generate_custom_report.py",
    "test:e2e:report:detailed": "python scripts/generate_detailed_report.py",
    "test:e2e:report:summary": "python scripts/generate_summary_report.py",
    "test:e2e:report:trends": "python scripts/generate_trends_report.py",
    "test:e2e:report:comparison": "python scripts/generate_comparison_report.py",
    "test:e2e:report:coverage": "python scripts/generate_coverage_report.py",
    "test:e2e:report:performance": "python scripts/generate_performance_report.py",
    "test:e2e:report:quality": "python scripts/generate_quality_report.py",
    "test:e2e:report:security": "python scripts/generate_security_report.py",
    "test:e2e:report:accessibility": "python scripts/generate_accessibility_report.py",
    "test:e2e:report:compliance": "python scripts/generate_compliance_report.py",
    "test:e2e:report:audit": "python scripts/generate_audit_report.py",
    "test:e2e:report:validation": "python scripts/generate_validation_report.py",
    "test:e2e:report:verification": "python scripts/generate_verification_report.py",
    "test:e2e:report:certification": "python scripts/generate_certification_report.py",
    "test:e2e:report:accreditation": "python scripts/generate_accreditation_report.py",
    "test:e2e:report:approval": "python scripts/generate_approval_report.py",
    "test:e2e:report:endorsement": "python scripts/generate_endorsement_report.py",
    "test:e2e:report:authorization": "python scripts/generate_authorization_report.py",
    "test:e2e:report:clearance": "python scripts/generate_clearance_report.py",
    "test:e2e:report:certification": "python scripts/generate_certification_report.py",
    "test:e2e:report:accreditation": "python scripts/generate_accreditation_report.py",
    "test:e2e:report:approval": "python scripts/generate_approval_report.py",
    "test:e2e:report:endorsement": "python scripts/generate_endorsement_report.py",
    "test:e2e:report:authorization": "python scripts/generate_authorization_report.py",
    "test:e2e:report:clearance": "python scripts/generate_clearance_report.py",
    "generate:api-client": "openapi-generator-cli generate -i docs/openapi.yaml -g typescript-axios -o ui/generated/api-client",
    "generate:api-types": "openapi-generator-cli generate -i docs/openapi.yaml -g typescript-axios -o ui/generated/api-types",
    "validate:openapi": "openapi-generator-cli validate -i docs/openapi.yaml",
    "sync:contracts": "npm run validate:openapi && npm run generate:api-client && npm run generate:api-types",
    "sync:types": "python scripts/sync-types.py",
    "generate:shared-types": "python -c \"from shared.types import save_typescript_types; save_typescript_types()\"",
    "validate:types": "python scripts/sync-types.py --validate-only",
    "sync:openapi": "python scripts/sync-openapi.py",
    "detect:breaking-changes": "python scripts/detect-breaking-changes.py",
    "version:bump-major": "python -c \"from shared.versioning import bump_major_version; print(bump_major_version())\"",
    "version:bump-minor": "python -c \"from shared.versioning import bump_minor_version; print(bump_minor_version())\"",
    "version:bump-patch": "python -c \"from shared.versioning import bump_patch_version; print(bump_patch_version())\"",
    "version:current": "python -c \"from shared.versioning import get_current_version; print(get_current_version())\"",
    "generate:changelog": "python -c \"from shared.versioning import generate_changelog; print(generate_changelog('1.0.0', '2.0.0'))\"",
    "create:migration-guide": "python -c \"from shared.versioning import create_migration_guide; print(create_migration_guide('1.0.0', '2.0.0'))\""
  },
  "keywords": [
    "e2e",
    "testing",
    "playwright",
    "omni-writer",
    "automation"
  ],
  "author": "Omni Writer Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@axe-core/playwright": "^4.8.0",
    "playwright": "^1.40.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "zod": "^3.22.4",
    "axios": "^1.6.0"
  },
  "dependencies": {
    "perf_hooks": "^1.0.0",
    "@tanstack/react-query": "^5.17.0",
    "@tanstack/react-query-devtools": "^5.17.0",
    "swr": "^2.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omni-writer/e2e-tests.git"
  },
  "bugs": {
    "url": "https://github.com/omni-writer/e2e-tests/issues"
  },
  "homepage": "https://github.com/omni-writer/e2e-tests#readme"
}
