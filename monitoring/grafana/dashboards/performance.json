{
  "dashboard": {
    "id": null,
    "title": "Omni Writer - Performance Dashboard",
    "tags": ["omni-writer", "performance", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "omni_writer_system_metrics{metric_name=\"health_score\"}",
            "legendFormat": "Health Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 80}
              ]
            },
            "unit": "percent",
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Active Alerts",
        "type": "stat",
        "targets": [
          {
            "expr": "omni_writer_alerts_total",
            "legendFormat": "{{severity}} - {{rule_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "omni_writer_system_metrics{metric_name=\"cpu_usage_percent\"}",
            "legendFormat": "CPU %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "omni_writer_system_metrics{metric_name=\"memory_usage_percent\"}",
            "legendFormat": "Memory %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 85},
                {"color": "red", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(omni_writer_requests_total[5m])",
            "legendFormat": "{{endpoint}} - {{method}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Request Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(omni_writer_request_duration_seconds_sum[5m]) / rate(omni_writer_request_duration_seconds_count[5m])",
            "legendFormat": "{{endpoint}} - {{method}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 2},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Generation Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(omni_writer_generations_total[5m])",
            "legendFormat": "{{model_type}} - {{status}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "generations/sec"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Generation Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(omni_writer_generation_duration_seconds_sum[5m]) / rate(omni_writer_generation_duration_seconds_count[5m])",
            "legendFormat": "{{model_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(omni_writer_cache_hits_total[5m])",
            "legendFormat": "Hits - {{cache_type}}"
          },
          {
            "expr": "rate(omni_writer_cache_misses_total[5m])",
            "legendFormat": "Misses - {{cache_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ops/sec"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(omni_writer_errors_total[5m])",
            "legendFormat": "{{error_type}} - {{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "errors/sec",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 11,
        "title": "Active Workers",
        "type": "stat",
        "targets": [
          {
            "expr": "omni_writer_active_workers",
            "legendFormat": "Active Workers"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 2}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 32}
      },
      {
        "id": 12,
        "title": "Queue Size",
        "type": "graph",
        "targets": [
          {
            "expr": "omni_writer_queue_size",
            "legendFormat": "{{queue_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 32}
      },
      {
        "id": 13,
        "title": "Disk Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "omni_writer_system_metrics{metric_name=\"disk_usage_percent\"}",
            "legendFormat": "Disk %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 32}
      },
      {
        "id": 14,
        "title": "Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "omni_writer_system_metrics{metric_name=\"network_bytes_sent\"}",
            "legendFormat": "Bytes Sent"
          },
          {
            "expr": "omni_writer_system_metrics{metric_name=\"network_bytes_recv\"}",
            "legendFormat": "Bytes Received"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "bytes"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 32}
      },
      {
        "id": 15,
        "title": "Process Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "omni_writer_system_metrics{metric_name=\"process_cpu_percent\"}",
            "legendFormat": "Process CPU %"
          },
          {
            "expr": "omni_writer_system_metrics{metric_name=\"process_memory_mb\"}",
            "legendFormat": "Process Memory MB"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40}
      },
      {
        "id": 16,
        "title": "Alert Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(omni_writer_alert_duration_seconds_sum[5m]) / rate(omni_writer_alert_duration_seconds_count[5m])",
            "legendFormat": "{{severity}} - {{rule_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "allValue": null,
          "current": {},
          "datasource": "Prometheus",
          "definition": "label_values(omni_writer_requests_total, endpoint)",
          "description": null,
          "error": null,
          "hide": 0,
          "includeAll": false,
          "label": "Endpoint",
          "multi": false,
          "name": "endpoint",
          "options": [],
          "query": "label_values(omni_writer_requests_total, endpoint)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        },
        {
          "allValue": null,
          "current": {},
          "datasource": "Prometheus",
          "definition": "label_values(omni_writer_generations_total, model_type)",
          "description": null,
          "error": null,
          "hide": 0,
          "includeAll": false,
          "label": "Model Type",
          "multi": false,
          "name": "model_type",
          "options": [],
          "query": "label_values(omni_writer_generations_total, model_type)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        }
      ]
    }
  }
} 